<div class="panel-group" id="accordion">



</div>

<script id="entry-template" type="text/x-handlebars-template">

    {{#each transacoes}}
        <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title">
                    <a data-toggle="collapse" data-parent="#accordion" href="#collapse1" class="">
                        <span class="glyphicon glyphicon-list" aria-hidden="true"></span> {{this.perfil}} siad </a>
                    </h4>
                </div>
            
                {{#each filhos}}
                <div id="collapse1" class="panel-collapse">
                    <div class="panel-body">
                        <div><span class="glyphicon glyphicon-menu-right" aria-hidden="true"></span> 
                            <a href="{{this.url}}" target="frameContent"> {{this.transacao}} </a>
                        </div>
                    </div>
                </div>

                
                {{/each}}
        </div>
    {{/each}}
</script>